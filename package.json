{
  "name": "hep-pug",
  "version": "2.0.2",
  "description": "A pug middleware for Hep or Koa that let render views easier",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-cov": "jest --coverage --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codinggirl/hep.git"
  },
  "keywords": [
    "hep",
    "rose",
    "koa",
    "koa-cms",
    "pug",
    "cms",
    "middleware",
    "middle",
    "rose",
    "egg",
    "eggjs"
  ],
  "author": "Richard Libre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codinggirl/hep-pug/issues"
  },
  "homepage": "https://github.com/codinggirl/hep-pug#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "jest": "^22.4.2"
  },
  "jest": {
    "testMatch": [
      "**/test/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": true,
    "testEnvironment": "node"
  }
}
